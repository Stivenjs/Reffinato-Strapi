{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "Address",
    "description": "User Address Information"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "companyName": {
      "type": "string"
    },
    "address": {
      "type": "string",
      "required": true
    },
    "addressLine2": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "country": {
      "type": "string",
      "required": true
    },
    "region": {
      "type": "string"
    },
    "zipCode": {
      "type": "string",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "isDefault": {
      "type": "boolean",
      "default": false
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::auth.auth",
      "inversedBy": "address"
    }
  }
}
